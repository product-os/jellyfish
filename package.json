{
  "name": "jellyfish",
  "version": "33.41.6",
  "codename": "cephea",
  "private": true,
  "description": "The Jellyfish Project",
  "homepage": "https://github.com/product-os/jellyfish",
  "repository": {
    "type": "git",
    "url": "git@github.com:product-os/jellyfish.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "scripts": {
    "postinstall": "husky install"
  },
  "deplint": {
    "files": [
      "apps/action-server/lib/**/*.js",
      "apps/server/*.js",
      "apps/server/graphql/**/*.js",
      "apps/server/http/**/*.js",
      "apps/server/socket/**/*.js"
    ],
    "modules": [
      "apps/server",
      "apps/action-server"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true,
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "lint-staged": {
    "*": [
      "./scripts/lint/check-filenames.sh"
    ],
    "apps/*": [
      "./scripts/lint/check-descriptions.sh",
      "./scripts/lint/check-apps.sh"
    ],
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "deplint"
    ],
    "*.sh": [
      "shellcheck"
    ],
    "*.{js,jsx,sh}": [
      "./scripts/lint/check-licenses.sh"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.67",
    "@balena/jellyfish-action-library": "^6.0.8",
    "@balena/jellyfish-chat-widget": "^5.0.31",
    "@balena/jellyfish-client-sdk": "^2.7.4",
    "@balena/jellyfish-core": "^0.2.40",
    "@balena/jellyfish-environment": "^2.3.13",
    "@balena/jellyfish-plugin-default": "^1.1.19",
    "@balena/jellyfish-queue": "^0.0.279",
    "@balena/jellyfish-sync": "^4.0.8",
    "@balena/jellyfish-uuid": "^1.0.42",
    "@balena/jellyfish-worker": "^2.2.10",
    "@octokit/plugin-retry": "^3.0.6",
    "@octokit/plugin-throttling": "^3.3.4",
    "@octokit/rest": "^18.0.12",
    "ava": "^3.14.0",
    "aws-sdk": "^2.816.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "blueimp-md5": "^2.18.0",
    "cli-spinner": "^0.2.10",
    "date-fns": "^2.16.1",
    "depcheck": "^1.3.1",
    "deplint": "^1.1.2",
    "errio": "^1.2.2",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "front-sdk": "^0.8.1",
    "husky": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "mermaid.cli": "^0.5.1",
    "mustache": "^4.1.0",
    "nock": "^13.0.5",
    "node-jose": "^2.0.0",
    "puppeteer": "^5.5.0",
    "random-words": "^1.1.1",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "shellcheck": "^0.4.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {}
}
