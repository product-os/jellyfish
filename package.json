    "history": "^4.7.2",
    "howler": "^2.0.12",
    "json-e": "^2.6.0",
    "localforage": "^1.6.0",
    "lodash": "^4.17.5",
    "mark.js": "^8.11.1",
    "moment": "^2.22.1",
    "object-hash": "^1.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-trello": "^1.31.3",
    "react-virtualized": "^9.20.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rendition": "^4.18.1",
    "reql-schema": "^1.0.0",
    "rethinkdb": "^2.3.3",
    "skhema": "^2.0.1",
