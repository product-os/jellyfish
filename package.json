    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true,
    "extensions": [
      "jsx"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ]
      }
    },
    "files": [
      "test/**/*.spec.{js,jsx}",
      "lib/**/*.spec.{js,jsx}",
      "apps/**/*.spec.{js,jsx}"
    ],
    "require": [
      "./test/browser-setup",
      "./test/register",
