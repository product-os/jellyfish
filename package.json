    "files": [
      "lib/**/*.{js,jsx}",
      "apps/action-server/**/*.js",
      "apps/server/**/*.js"
    ],
    "modules": [
      "apps/server",
      "apps/action-server",
      "lib/core",
      "lib/action-library",
      "lib/chat-widget",
      "lib/queue",
      "lib/ui-components",
      "lib/sync",
      "lib/worker"
    ]
  },
