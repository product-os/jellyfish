{
  "name": "jellyfish",
  "version": "42.3.31",
  "homepage": "https://github.com/product-os/jellyfish",
  "description": "The Jellyfish Project",
  "codename": "#STPLTG",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:product-os/jellyfish.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx scripts test && npm run lint:check && npm run lint:deps && npm run lint:livechat && npm run lint:server && npm run lint:ui",
    "lint:check": "./scripts/lint/check-filenames.sh && shellcheck scripts/*.sh scripts/*/*.sh",
    "lint:catch": "catch-uncommitted --skip-node-versionbot-changes --exclude=VERSION,.versionbot/,.balena/,Dockerfile.template",
    "lint:deps": "deplint && depcheck --ignore-bin-package --ignores=@balena/jellyfish-*,shellcheck",
    "lint:livechat": "(cd apps/livechat && npm run lint)",
    "lint:server": "(cd apps/server && npm run lint)",
    "lint:ui": "(cd apps/ui && npm run lint)",
    "test:unit": "npm run test:unit:ui && npm run test:unit:server",
    "test:unit:ui": "(cd apps/ui && npm run test:unit)",
    "test:unit:server": "(cd apps/server && npm run test:unit)",
    "test:e2e:sdk": "ava ./test/e2e/sdk/*.spec.js",
    "test:e2e:livechat": "ava ./test/e2e/livechat/**/*.spec.js",
    "test:e2e:ui": "INTEGRATION_OUTREACH_APP_ID= ava ./test/e2e/ui/**/*.spec.js",
    "test:e2e:server": "ava ./test/e2e/server/**/*.spec.js",
    "test:integration:server": "(cd apps/server && npm run test:integration)",
    "docs": "(cd apps/livechat && npm run doc); (cd apps/server && npm run doc); (cd apps/ui && npm run doc)",
    "docs:diagram": "nomnoml docs/diagrams/architecture.noml > docs/assets/architecture.svg",
    "docs:summary": "./scripts/architecture-summary.sh > ARCHITECTURE.md",
    "clean": "rimraf apps/*/packages/*",
    "postinstall": "(cd apps/livechat && npm i); (cd apps/server && npm i); (cd apps/ui && npm i)",
    "push": "./scripts/push.sh",
    "push:lib": "./scripts/push-lib.js",
    "build:livechat": "(cd apps/livechat && npm run build)",
    "build:server": "(cd apps/server && npm run build)",
    "build:ui": "(cd apps/ui && npm run build)",
    "dev:livechat": "(cd apps/livechat && npm run dev)",
    "dev:server": "(cd apps/server && npm run dev)",
    "dev:ui": "(cd apps/ui && npm run dev)"
  },
  "deplint": {
    "files": [
      "apps/server/lib/**/*.ts"
    ],
    "modules": [
      "apps/server"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true,
    "failFast": true,
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "deplint"
    ],
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ],
    "*.sh": [
      "shellcheck"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@balena/jellyfish-action-library": "^16.2.0",
    "@balena/jellyfish-chat-widget": "^12.5.102",
    "@balena/jellyfish-client-sdk": "^7.0.33",
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-core": "^8.2.7",
    "@balena/jellyfish-environment": "^6.0.8",
    "@balena/jellyfish-plugin-balena-api": "^1.0.239",
    "@balena/jellyfish-plugin-base": "^2.2.22",
    "@balena/jellyfish-plugin-channels": "^1.1.438",
    "@balena/jellyfish-plugin-default": "^21.2.14",
    "@balena/jellyfish-plugin-discourse": "^1.1.129",
    "@balena/jellyfish-plugin-flowdock": "^1.0.268",
    "@balena/jellyfish-plugin-front": "^1.3.147",
    "@balena/jellyfish-plugin-github": "^1.11.284",
    "@balena/jellyfish-plugin-outreach": "^1.0.386",
    "@balena/jellyfish-plugin-product-os": "^2.9.45",
    "@balena/jellyfish-plugin-typeform": "^4.0.31",
    "@balena/jellyfish-queue": "^1.0.395",
    "@balena/jellyfish-sync": "^6.3.0",
    "@balena/jellyfish-ui-components": "^13.1.19",
    "@balena/jellyfish-worker": "^10.1.30",
    "@balena/lint": "^6.2.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "ava": "^3.15.0",
    "aws-sdk": "^2.1053.0",
    "bluebird": "^3.7.2",
    "catch-uncommitted": "^2.0.0",
    "cli-spinner": "^0.2.10",
    "date-fns": "^2.28.0",
    "depcheck": "^1.4.3",
    "deplint": "^1.1.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "nock": "^13.2.1",
    "node-jose": "^2.0.0",
    "nomnoml": "^1.4.0",
    "puppeteer": "^13.0.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-01-13T15:58:16.402Z"
  },
