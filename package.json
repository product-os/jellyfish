{
  "name": "jellyfish",
  "version": "2.3.0",
  "private": true,
  "description": "The Jellyfish Project",
  "main": "lib/server.js",
  "homepage": "https://github.com/resin-io/jellyfish",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io/jellyfish.git"
  },
  "scripts": {
    "start": "DEBUG=jellyfish:* node lib/index.js",
    "start:profiler": "DEBUG=jellyfish:* node profiler.js",
    "test": "npm run lint && npm run test:command",
    "test:command": "nyc --reporter=lcov ava-ts && TEST_DB_HOST=localhost TEST_PROXY_PORT=9999 TEST_DB_PORT=28015 API_URL='http://localhost:8000' NODE_ENV=test nyc --reporter=lcov ava",
    "posttest": "./scripts/scrub-test-databases.js",
    "lint": "npm run eslint && npm run tslint",
    "eslint": "eslint lib test stress webpack.config.js",
    "eslint:fix": "eslint --fix lib test webpack.config.js",
    "tslint": "tslint -t stylish \"lib/**/*.ts\" \"lib/**/*.tsx\" -e \"lib/sdk/node_modules/**\" -e \"lib/sdk/dist/**\"",
    "tslint:fix": "tslint -t stylish --fix \"lib/**/*.ts\" \"lib/**/*.tsx\" -e \"lib/sdk/node_modules/**\" -e \"lib/sdk/dist/**\"",
    "build:ui": "NODE_ENV=production webpack",
    "dev:ui": "NODE_ENV=dev API_URL='http://localhost:8000/' webpack-dev-server --color",
    "heroku-postbuild": "npm run build:ui",
    "versionist": "versionist",
    "addgittag": "./scripts/add-version-tag.js"
  },
  "ava": {
    "timeout": "5m",
    "verbose": true,
    "concurrency": 3,
    "failFast": true,
    "require": [
      "./test/test-helpers/ui-setup.js"
    ],
    "files": [
      "test/**/*.spec.ts",
      "test/**/*.spec.js"
    ],
    "babel": {
      "presets": [
        "react"
      ]
    }
  },
  "author": "Resin Inc. <hello@resin.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/url-regex": "^4.0.0",
    "ava": "^0.22.0",
    "ava-ts": "^0.25.0",
    "babel-preset-react": "^6.24.1",
    "cache-loader": "^1.2.2",
    "cli-spinner": "^0.2.8",
    "copy-webpack-plugin": "^4.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsdoc": "^3.1.2",
    "eslint-plugin-lodash": "^2.4.5",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.0.6",
    "jsdom": "^11.8.0",
    "marky": "^1.2.0",
    "nyc": "^11.4.1",
    "percentile": "^1.2.1",
    "randomstring": "^1.1.5",
    "request": "^2.85.0",
    "source-map-loader": "^0.2.3",
    "thread-loader": "^1.1.5",
    "ts-loader": "^4.0.1",
    "ts-node": "^6.0.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.2",
    "v8-profiler": "^5.7.0",
    "versionist": "^2.13.0",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@resin.io/jellyfish-sdk": "file:./lib/sdk",
    "@types/blueimp-md5": "^2.7.0",
    "@types/es6-promise": "^3.3.0",
    "@types/history": "^4.6.2",
    "@types/howler": "^2.0.5",
    "@types/json-schema": "^6.0.1",
    "@types/lodash": "^4.14.104",
    "@types/mark.js": "^8.11.1",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.19",
    "@types/react-virtualized": "^9.18.3",
    "@types/socket.io-client": "^1.4.32",
    "@webscopeio/react-textarea-autocomplete": "^2.1.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
