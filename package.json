{
  "name": "jellyfish",
  "version": "42.2.3",
  "homepage": "https://github.com/product-os/jellyfish",
  "description": "The Jellyfish Project",
  "codename": "#STPLTG",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:product-os/jellyfish.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx scripts test && npm run lint:check && npm run lint:deps",
    "lint:check": "./scripts/lint/check-filenames.sh && shellcheck scripts/*.sh scripts/*/*.sh deploy-templates/*.sh",
    "lint:deps": "deplint && depcheck --ignore-bin-package --ignores=@balena/jellyfish-*",
    "lint:catch": "catch-uncommitted --skip-node-versionbot-changes --exclude=VERSION,.versionbot/",
    "test:e2e:sdk": "ava ./test/e2e/sdk/*.spec.js",
    "test:e2e:livechat": "ava ./test/e2e/livechat/**/*.spec.js",
    "test:e2e:ui": "INTEGRATION_OUTREACH_APP_ID= ava ./test/e2e/ui/**/*.spec.js",
    "test:e2e:server": "ava ./test/e2e/server/**/*.spec.js",
    "docs": "(cd apps/livechat && npm run doc); (cd apps/server && npm run doc); (cd apps/ui && npm run doc)",
    "docs:diagram": "mmdc -i docs/diagrams/architecture.mmd -o docs/assets/architecture.png -w 2560 -H 1600",
    "docs:summary": "./scripts/architecture-summary.sh > ARCHITECTURE.md",
    "clean": "rimraf apps/*/packages/*",
    "postinstall": "(cd apps/livechat && npm i); (cd apps/server && npm i); (cd apps/ui && npm i)",
    "compose:build": "docker-compose build",
    "compose:up": "docker-compose up",
    "compose:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "compose:monitor": "docker-compose -f docker-compose.yml -f docker-compose.monitor.yml up",
    "compose:database": "docker-compose up postgres redis haproxy"
  },
  "deplint": {
    "files": [
      "apps/server/lib/**/*.ts"
    ],
    "modules": [
      "apps/server"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true,
    "failFast": true,
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "deplint"
    ],
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ],
    "*.sh": [
      "shellcheck"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@balena/jellyfish-action-library": "^15.1.186",
    "@balena/jellyfish-chat-widget": "^12.5.69",
    "@balena/jellyfish-client-sdk": "^7.0.23",
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-core": "^8.1.44",
    "@balena/jellyfish-environment": "^5.2.2",
    "@balena/jellyfish-plugin-balena-api": "^1.0.197",
    "@balena/jellyfish-plugin-base": "^2.1.320",
    "@balena/jellyfish-plugin-channels": "^1.1.400",
    "@balena/jellyfish-plugin-default": "^21.1.143",
    "@balena/jellyfish-plugin-discourse": "^1.1.112",
    "@balena/jellyfish-plugin-flowdock": "^1.0.238",
    "@balena/jellyfish-plugin-front": "^1.3.118",
    "@balena/jellyfish-plugin-github": "^1.11.231",
    "@balena/jellyfish-plugin-outreach": "^1.0.331",
    "@balena/jellyfish-plugin-product-os": "^2.9.2",
    "@balena/jellyfish-plugin-typeform": "^3.0.413",
    "@balena/jellyfish-queue": "^1.0.380",
    "@balena/jellyfish-sync": "^6.1.126",
    "@balena/jellyfish-ui-components": "^13.1.2",
    "@balena/jellyfish-worker": "^10.1.0",
    "@balena/lint": "^6.2.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "ava": "^3.15.0",
    "aws-sdk": "^2.1043.0",
    "bluebird": "^3.7.2",
    "catch-uncommitted": "^2.0.0",
    "cli-spinner": "^0.2.10",
    "date-fns": "^2.27.0",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "mermaid.cli": "^0.5.1",
    "nock": "^13.2.1",
    "node-jose": "^2.0.0",
    "puppeteer": "^12.0.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "shellcheck": "^1.0.0",
    "simple-git-hooks": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-12-10T02:02:26.832Z"
  }
}
