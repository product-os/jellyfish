{
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es6",
  },
  "include": [
    "./**/*.ts"
  ]
}

