{
  "name": "jellyfish-server",
  "version": "0.0.1",
  "codename": "cephea",
  "private": true,
  "description": "The Jellyfish Server App",
  "engines": {
    "node": ">=14.2.0"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test:unit": "jest ./test/unit/.*.spec.ts$",
    "test:integration": "CI=1 SERVER_HOST=http://localhost jest --runInBand --testTimeout=120000 --forceExit ./test/integration/.*.spec.ts$",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run test:unit && npm run test:integration",
    "doc": "typedoc lib/",
    "prepack": "npm run build",
    "start": "node --abort-on-uncaught-exception --stack-trace-limit=100 build/index.js",
    "dev": "nodemon --inspect=0.0.0.0 ./lib/index.ts"
  },
  "dependencies": {
    "@balena/jellyfish-action-library": "^15.1.195",
    "@balena/jellyfish-assert": "^1.1.103",
    "@balena/jellyfish-core": "^8.1.53",
    "@balena/jellyfish-environment": "^6.0.2",
    "@balena/jellyfish-logger": "^3.0.167",
    "@balena/jellyfish-metrics": "^1.0.438",
    "@balena/jellyfish-plugin-balena-api": "^1.0.205",
    "@balena/jellyfish-plugin-base": "^2.2.1",
    "@balena/jellyfish-plugin-channels": "^1.1.408",
    "@balena/jellyfish-plugin-default": "^21.1.149",
    "@balena/jellyfish-plugin-discourse": "^1.1.116",
    "@balena/jellyfish-plugin-flowdock": "^1.0.245",
    "@balena/jellyfish-plugin-front": "^1.3.126",
    "@balena/jellyfish-plugin-github": "^1.11.242",
    "@balena/jellyfish-plugin-outreach": "^1.0.342",
    "@balena/jellyfish-plugin-product-os": "^2.9.10",
    "@balena/jellyfish-plugin-typeform": "^4.0.9",
    "@balena/jellyfish-queue": "^1.0.389",
    "@balena/jellyfish-sync": "^6.1.133",
    "@balena/jellyfish-worker": "^10.1.11",
    "@balena/socket-prometheus-metrics": "^0.0.3",
    "aws-sdk": "^2.1046.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "errio": "^1.2.2",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.1",
    "json-e": "^4.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mkdirp-promise": "^5.0.1",
    "multer": "^1.4.4",
    "response-time": "^2.3.2",
    "skhema": "^5.3.4",
    "socket.io": "^4.4.0",
    "socket.io-redis": "^6.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-client-sdk": "^6.0.3",
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-types": "^1.2.40",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.0",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.2",
    "jest": "^27.4.4",
    "nock": "^13.2.1",
    "nodemon": "^2.0.15",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3"
  }
}
