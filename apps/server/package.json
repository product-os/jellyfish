{
  "name": "jellyfish-server",
  "version": "0.0.1",
  "codename": "cephea",
  "private": true,
  "description": "The Jellyfish Server App",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test:integration": "CI=1 SERVER_HOST=http://localhost jest --runInBand --testTimeout=120000 --forceExit ./test/integration/.*.spec.ts$",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run test:integration",
    "doc": "typedoc lib/",
    "prepack": "npm run build",
    "start": "node --abort-on-uncaught-exception --stack-trace-limit=100 build/index.js",
    "dev": "NODE_ENV=development nodemon --inspect=0.0.0.0 ./lib/index.ts"
  },
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.29",
    "@balena/jellyfish-environment": "^11.0.3",
    "@balena/jellyfish-logger": "^5.0.31",
    "@balena/jellyfish-metrics": "^2.0.76",
    "@balena/jellyfish-plugin-balena-api": "^2.0.142",
    "@balena/jellyfish-plugin-channels": "^2.0.113",
    "@balena/jellyfish-plugin-default": "^24.0.110",
    "@balena/jellyfish-plugin-discourse": "^3.0.0",
    "@balena/jellyfish-plugin-flowdock": "^2.0.144",
    "@balena/jellyfish-plugin-front": "^2.0.174",
    "@balena/jellyfish-plugin-github": "^2.2.65",
    "@balena/jellyfish-plugin-outreach": "^2.0.149",
    "@balena/jellyfish-plugin-product-os": "^5.0.21",
    "@balena/jellyfish-plugin-typeform": "^5.2.13",
    "@balena/jellyfish-worker": "^23.0.17",
    "@balena/socket-prometheus-metrics": "^0.0.3",
    "autumndb": "^19.1.41",
    "aws-sdk": "^2.1130.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "errio": "^1.2.2",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "is-uuid": "^1.0.2",
    "json-e": "^4.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "response-time": "^2.3.2",
    "skhema": "^6.0.6",
    "socket.io": "^4.5.0",
    "socket.io-redis": "^6.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-client-sdk": "^10.4.0",
    "@balena/jellyfish-config": "^2.0.6",
    "@balena/jellyfish-types": "^2.0.5",
    "@balena/lint": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/response-time": "^2.3.5",
    "@types/uuid": "^8.3.4",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.3",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "nodemon": "^2.0.16",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  }
}
