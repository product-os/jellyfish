{
  "name": "jellyfish-server",
  "version": "0.0.1",
  "codename": "cephea",
  "private": true,
  "description": "The Jellyfish Server App",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test:unit": "jest ./test/unit/.*.spec.ts$",
    "test:integration": "CI=1 SERVER_HOST=http://localhost jest --runInBand --testTimeout=120000 --forceExit ./test/integration/.*.spec.ts$",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run test:unit && npm run test:integration",
    "doc": "typedoc lib/",
    "prepack": "npm run build",
    "start": "node --abort-on-uncaught-exception --stack-trace-limit=100 build/index.js",
    "dev": "nodemon --inspect=0.0.0.0 ./lib/index.ts"
  },
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.14",
    "@balena/jellyfish-core": "^14.3.5",
    "@balena/jellyfish-environment": "^6.1.1",
    "@balena/jellyfish-logger": "^4.0.36",
    "@balena/jellyfish-metrics": "^2.0.37",
    "@balena/jellyfish-plugin-balena-api": "^2.0.19",
    "@balena/jellyfish-plugin-channels": "^2.0.18",
    "@balena/jellyfish-plugin-default": "^23.2.1",
    "@balena/jellyfish-plugin-discourse": "^2.0.18",
    "@balena/jellyfish-plugin-flowdock": "^2.0.12",
    "@balena/jellyfish-plugin-front": "^2.0.17",
    "@balena/jellyfish-plugin-github": "^2.0.17",
    "@balena/jellyfish-plugin-outreach": "^2.0.15",
    "@balena/jellyfish-plugin-product-os": "^4.0.15",
    "@balena/jellyfish-plugin-typeform": "^5.0.13",
    "@balena/jellyfish-queue": "^4.0.39",
    "@balena/jellyfish-worker": "^18.6.18",
    "@balena/socket-prometheus-metrics": "^0.0.3",
    "aws-sdk": "^2.1071.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "errio": "^1.2.2",
    "express": "^4.17.2",
    "express-basic-auth": "^1.2.1",
    "json-e": "^4.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mkdirp-promise": "^5.0.1",
    "multer": "^1.4.4",
    "response-time": "^2.3.2",
    "skhema": "^6.0.2",
    "socket.io": "^4.4.1",
    "socket.io-redis": "^6.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-client-sdk": "^8.3.7",
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-types": "^2.0.4",
    "@balena/lint": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.15",
    "@types/response-time": "^2.3.5",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.3",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
}
