{
  "name": "jellyfish-server",
  "version": "0.0.1",
  "codename": "snappy",
  "private": true,
  "description": "The Jellyfish Server App",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test",
    "lint:fix": "balena-lint --fix lib test",
    "test:integration": "CI=1 SERVER_HOST=http://localhost jest --runInBand --testTimeout=120000 --forceExit ./test/integration/.*.spec.ts$",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run test:integration",
    "prepack": "npm run build",
    "start": "node --abort-on-uncaught-exception --stack-trace-limit=100 build/index.js",
    "dev": "NODE_ENV=development nodemon --inspect=0.0.0.0 ./lib/index.ts"
  },
  "dependencies": {
    "@balena/jellyfish-assert": "^1.11.2",
    "@balena/jellyfish-environment": "^21.0.0",
    "@balena/jellyfish-logger": "^6.0.1",
    "@balena/jellyfish-plugin-balena-api": "^6.8.7",
    "@balena/jellyfish-plugin-discourse": "^6.3.3",
    "@balena/jellyfish-plugin-front": "^6.9.1",
    "@balena/jellyfish-plugin-github": "^9.8.7",
    "@balena/jellyfish-worker": "^51.0.2",
    "autumndb": "^28.1.1",
    "aws-sdk": "^2.1354.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "errio": "^1.2.2",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "handover-lib": "^0.2.0",
    "is-uuid": "^1.0.2",
    "json-e": "^4.4.3",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "skhema": "^6.0.6",
    "socket.io": "^4.6.1",
    "socket.io-redis": "^6.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@balena/jellyfish-client-sdk": "^13.11.3",
    "@balena/lint": "^6.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "catch-uncommitted": "^2.0.0",
    "jest": "^27.5.1",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "request": "^2.88.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.8.4"
  }
}
