{
  "name": "jellyfish-server",
  "version": "0.0.1",
  "codename": "snappy",
  "private": true,
  "description": "The Jellyfish Server App",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test",
    "lint:fix": "balena-lint --fix lib test",
    "test:integration": "CI=1 SERVER_HOST=http://localhost jest --runInBand --testTimeout=120000 --forceExit ./test/integration/.*.spec.ts$",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run test:integration",
    "doc": "typedoc lib/",
    "prepack": "npm run build",
    "start": "node --abort-on-uncaught-exception --stack-trace-limit=100 build/index.js",
    "dev": "NODE_ENV=development nodemon --inspect=0.0.0.0 ./lib/index.ts"
  },
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.50",
    "@balena/jellyfish-environment": "^12.3.5",
    "@balena/jellyfish-logger": "^5.1.21",
    "@balena/jellyfish-metrics": "^2.0.115",
    "@balena/jellyfish-plugin-balena-api": "^6.0.28",
    "@balena/jellyfish-plugin-discourse": "^6.0.18",
    "@balena/jellyfish-plugin-flowdock": "^5.0.70",
    "@balena/jellyfish-plugin-front": "^6.0.14",
    "@balena/jellyfish-plugin-github": "^8.0.10",
    "@balena/jellyfish-plugin-incidents": "^7.3.25",
    "@balena/jellyfish-plugin-outreach": "^5.0.24",
    "@balena/jellyfish-plugin-typeform": "^10.0.19",
    "@balena/jellyfish-worker": "^32.3.15",
    "@balena/socket-prometheus-metrics": "^0.0.3",
    "autumndb": "^21.2.1",
    "aws-sdk": "^2.1184.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "errio": "^1.2.2",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "is-uuid": "^1.0.2",
    "json-e": "^4.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "skhema": "^6.0.6",
    "socket.io": "^4.5.1",
    "socket.io-redis": "^6.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-client-sdk": "^13.0.18",
    "@balena/lint": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "catch-uncommitted": "^2.0.0",
    "jest": "^27.5.1",
    "nock": "^13.2.6",
    "nodemon": "^2.0.16",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.1",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4"
  }
}
