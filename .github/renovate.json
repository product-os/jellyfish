{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [],
  "enabledManagers": [
    "npm"
  ],
  "rangeStrategy": "bump",
  "commitMessagePrefix": "patch:",
  "prHourlyLimit": 0,
  "labels": [
    "dependencies"
  ],
  "ignoreDeps": [
    "monaco-editor",
    "monaco-editor-webpack-plugin"
  ],
  "packageRules": [
    {
      "groupName": "non-major",
      "updateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    }
  ],
  "encrypted": {
    "npmToken": ""
  }
}
