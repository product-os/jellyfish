{
  "extends": [
    "config:base"
  ],
  "enabledManagers": [
    "npm"
  ],
  "rangeStrategy": "bump",
  "commitMessagePrefix": "patch:",
  "prHourlyLimit": 0,
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "groupName": "non-major",
      "updateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "groupName": "dev-major",
      "depTypeList": [
        "devDependencies"
      ],
      "updateTypes": [
        "major"
      ],
      "automerge": true
    }
  ],
  "encrypted": {
    "npmToken": ""
  }
}
